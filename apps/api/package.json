    "@connectrpc/connect-express": "^2.0.2",
    "@connectrpc/connect-node": "^2.0.2",
    "@prisma/client": "^5.22.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.9.0",
    "eslint": "^9.14.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.12.0",
    "tsx": "^4.19.2",
